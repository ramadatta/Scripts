git clone https://github.com/your_username/bioconda-recipes.git
cd bioconda-recipes
git remote add upstream https://github.com/bioconda/bioconda-recipes.git


cd bioconda-recipes/recipes/
mkdir cpgeneprofiler
cp meta.yaml cpgeneprofiler
cp build.sh cpgeneprofiler

https://bioconda.github.io/contributor/workflow.html

 1975  git checkout master
 1979  git pull upstream master
 1980  git push origin master
 1981  git checkout -b update_my_recipe
 1982  cd recipes
 1983  conda skeleton cran cpgeneprofiler
 1987  git add cpgeneprofiler
 1988  git diff
 1989  git status
 1992  git add cpgeneprofiler
 1993  git commit "adding cpgeneprofiler"
 1996  git push --set-upstream origin update_my_recipe


